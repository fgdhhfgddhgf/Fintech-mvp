{
  "name": "FinTrustAI",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:frontend\"",
    "dev:frontend": "wait-on http://localhost:3001/health && vite",
    "dev:server": "cd server && npx prisma generate && tsx watch src/index.ts",
    "build": "npm run build:server && tsc -b && vite build",
    "build:frontend": "tsc -b && vite build",
    "build:server": "cd server && npx prisma generate && tsc",
    "lint": "eslint .",
    "test": "echo \"No tests configured\" && exit 0",
    "preview": "vite preview",
    "db:migrate": "cd server && prisma migrate dev",
    "db:push": "cd server && prisma db push",
    "db:migrate:prod": "cd server && prisma migrate deploy",
    "db:seed": "cd server && tsx prisma/seed.ts",
    "db:studio": "cd server && prisma studio",
    "start": "cd server && node dist/index.js"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@tremor/react": "^3.18.7",
    "axios": "^1.13.4",
    "axos": "^0.0.1",
    "bcryptjs": "^2.4.3",
    "bs58": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "express-validator": "^7.2.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "openai": "^4.73.0",
    "pg": "^8.13.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "recharts": "^2.14.1",
    "request": "^2.88.2",
    "tweetnacl": "^1.0.3",
    "uuid": "^10.0.0",
    "viem": "^2.45.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.9.0",
    "@types/pg": "^8.11.10",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.3",
    "concurrently": "^9.1.2",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^15.12.0",
    "postcss": "^8.4.49",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.15",
    "tsx": "^4.19.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.1",
    "wait-on": "^8.0.1"
  }
}
